));\n      if (this.state.count === 10) {\n        clearInterval(this.intervalid);\n        document.getElementById(\"on_off\").disabled = \"\";\n      }\n    }, 500);\n  }\n\n  Cal_ans_num() {\n    let arr = this.state.all;\n    let sum = function (arr) {\n      return arr.reduce(function (prev, current, i, arr) {\n        return prev + current;\n      });\n    };\n    return sum(arr);\n  }\n\n  showAns(e) {\n    if (this.state.all.length > 0) {\n      this.setState((state) => ({\n        ans: this.Cal_ans_num(),\n        num: 0,\n      }));\n    } else {\n      this.setState((state) => ({ans: 0}));\n    }\n  }\n\n  getFireData() {\n    let db = firebase.database();\n    let ref = db.ref(\"sample/\");\n    let self = this;\n    ref\n      .orderByKey()\n      .limitToFirst(1)\n      .on(\"value\", (snapshot) => {\n        self.setState({\n          data: snapshot.val(),\n        });\n      });\n\n    console.log(this.state.data[0]);\n    return this.state.data[0];\n  }\n\n  render() {\n    return (\n      <div className=\"First\">\n        <div className=\"App-header\">\n          <p className=\"Num_Display\">{this.state.num}</p>\n          <div className=\"Second\">\n            <div>\n              <select className=\"Speed_Button\" defaultValue={\"DEFAULT\"}>\n                <option value=\"DEFAULT\" disabled>\n                  Choose game level\n                </option>\n                <option value=\"1\">Lv.1</option>\n                <option value=\"2\">Lv.2</option>\n                <option value=\"3\">Lv.3</option>\n              </select>\n            </div>\n          </div>\n\n          <div className=\"Third\">\n            <button className=\"Click_Box\" onClick={this.doAction} id=\"on_off\">\n              Game Start!!\n            </button>\n          </div>\n\n          <div className=\"Fourth\">\n            <button className=\"ans_button\" onClick={this.showAns}>\n              SHOW ANSWER\n            </button>\n            <br />\n          </div>\n\n          <div className=\"Fifth\">\n            <table className=\"res\">\n              <tbody>\n                <tr>\n                  <td>{this.state.ans}</td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default App;\n",{"ruleId":"15","replacedBy":"16"},{"ruleId":"17","replacedBy":"18"},{"ruleId":"19","severity":1,"message":"20","line":35,"column":5,"nodeType":"21","endLine":35,"endColumn":15},"no-native-reassign",["22"],"no-negated-in-lhs",["23"],"react/no-direct-mutation-state","Do not mutate state directly. Use setState().","MemberExpression","no-global-assign","no-unsafe-negation"]